---
to: "<%=appName%>/docker-compose.yaml"
---
version: "3.7"

services:
  app:
    build: .
    ports:
      - "80:80"
    env_file:
      - ./<%=appName%>.env
    environment:
      - ARDB_SERVICE_HOST=ardb

  ardb:
    image: lupino/ardb-server
    ports:
      - "16379:16379"
    env_file:
      - ./<%=appName%>.env
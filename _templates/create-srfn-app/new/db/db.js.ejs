---
    to: "<%=userApiEnabled ? `${appName}/db/db.js` : null%>"
---
const redish = require( '@srfnstack/redish' )
const redis = require( 'redis' )

const client = redis.createClient( { host: process.env.REDIS_SERVICE_HOST || 'localhost', port: 6379 } )
client.on( 'error', console.error )
const db = redish.createDb( client )
module.exports = {db}
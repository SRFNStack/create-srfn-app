    "passport-jwt": "<%= locals.passportJwtVersion || '4.0.0' %>",
    "sendmail": "<%= locals.sendmailVersion || '1.6.1' %>",
    "uuid": "<%= locals.uuidVersion || '8.3.2' %>",
